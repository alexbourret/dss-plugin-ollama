{
    "meta": {
        "label": "Prediction",
        "description": "",
        "icon": "icon-magic"
    },
    "kind": "PYTHON",
    "paramsPythonSetup": "browse_models.py",
    "selectableFromDataset": "input_A_role",
    "inputRoles": [
        {
            "name": "input_A_role",
            "label": "input A displayed name",
            "description": "what input A means",
            "arity": "UNARY",
            "required": true,
            "acceptsDataset": true
        }
    ],

    "outputRoles": [
        {
            "name": "main_output",
            "label": "main output displayed name",
            "description": "what main output means",
            "arity": "UNARY",
            "required": false,
            "acceptsDataset": true
        }
    ],
    "params": [
        {
            "name": "prompt_source",
            "label": "Prompt source",
            "type": "SELECT",
            "description": "",
            "selectChoices": [
                {
                    "value": "column",
                    "label": "Column"
                },
                {
                    "value": "text",
                    "label": "Text box"
                }
            ]
        },
        {
            "name": "prompt",
            "label": "Prompt template",
            "type": "TEXTAREA",
            "description": "",
            "visibilityCondition": "model.prompt_source=='text'"
        },
        {
            "name": "prompt_column",
            "label": "Prompt column",
            "type": "COLUMN",
            "description": "",
            "columnRole": "input_A_role",
            "visibilityCondition": "model.prompt_source=='column'"
        },
        {
            "name": "model_select",
            "label": "Select model",
            "type": "SELECT",
            "getChoicesFromPython": true
        },
        {
            "name": "model_name",
            "label": "Model name",
            "type": "STRING",
            "description": "",
            "defaultValue": "mistral",
            "visibilityCondition": "model.model_select==None"
        },
        {
            "name": "model_column",
            "label": "Model column",
            "type": "COLUMN",
            "description": "",
            "columnRole": "input_A_role",
            "visibilityCondition": "model.model_select=='dku_column_select'"
        },
        {
            "name": "image_column",
            "label": "Image column",
            "type": "COLUMN",
            "description": "Must contain the URL of an image",
            "columnRole": "input_A_role"
        },
        {
            "name": "display_advanced_parameters",
            "label": " ",
            "description": "Advanced parameters",
            "type": "BOOLEAN"
        },
        {
            "name": "server_url",
            "label": "URL to Ollama instance",
            "type": "STRING",
            "description": "",
            "defaultValue": "http://localhost:11434",
            "visibilityCondition": "model.display_advanced_parameters"
        },
        {
            "name": "output_format",
            "label": "Output format",
            "type": "SELECT",
            "selectChoices": [
                {
                    "label": "Not specified"
                },
                {
                    "value": "json",
                    "label": "JSON"
                }
            ],
            "visibilityCondition": "model.display_advanced_parameters"
        }
    ],
    "resourceKeys": []
}
